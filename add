import java.util.Scanner;

class Add
{
       public static void main(String args[])
       {
            int x,y,sum;
            Scanner obj = new Scanner(System.in);
            System.out.println("type first number");
            x = obj.nextInt();
            System.out.println("type another number");
            y=obj.nextInt();
            sum = x+y;
            System.out.println("sum is: "+sum);
       }
}
        
